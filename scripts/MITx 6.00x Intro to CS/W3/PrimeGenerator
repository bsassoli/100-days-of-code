def genPrimes():
    n = 2
    primes = []
    def check_is_prime(n):
        l = [(n %i) !=0 for i in range(2,n)]
        a = int(sum(l))
        return a == n-2
    while True:
        if check_is_prime(n):
            primes.append(n)
            yield(n)
        n+=1
a = genPrimes()
for i in range(3,18):
    print(a.__next__())
